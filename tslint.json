{
    "extends": [
        "tslint:latest"
    ],
    "rules": {
        "no-implicit-dependencies": [
            true,
            "dev"
        ],
        "no-shadowed-variable": true,
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "space-before-function-paren": [
            true,
            {
                "anonymous": "always",
                "named": "never",
                "asyncArrow": "always"
            }
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": true
    }
}
